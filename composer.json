{
    "name": "dataworkstr/revisionable",
    "description": "Nice and easy way to handle revisions of your MongoDB.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dataworkstr/revisionable/issues",
        "source": "https://github.com/dataworkstr/revisionable"
    },
    "keywords": [
        "laravel",
        "lumen",
        "eloquent",
        "revisionable",
        "revision",
        "history",
        "mongodb",
        "document"
    ],
    "authors": [
        {
            "name": "Engin Kartal",
            "email": "admin@yellow.com.tr",
            "homepage": "https://yellowpages.com.tr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "mongodb/mongodb": "^1.0.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0@dev",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "*",
        "squizlabs/php_codesniffer": "~2.0",
        "illuminate/database": "5.2.*",
        "illuminate/auth": "5.2.*",
        "cartalyst/sentry": "~2.0",
        "tymon/jwt-auth": "^0.5.3"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Sofa\\Revisionable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofa\\Revisionable\\Tests\\": "tests"
        }
    }
}
